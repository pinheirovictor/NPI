d-icon=\"person\" type=\"number\" v-model=\"matAluno\" required />\n                            <v-text-field label=\"Matricula do Curso\" name=\"matCurso\" prepend-icon=\"school\" type=\"text\" v-model=\"matCurso\" required />\n                        </v-card-text>\n                        <v-card-actions>\n                            <v-btn color=\"error\" type=\"reset\" large>Cancelar</v-btn>\n                            <v-spacer /><v-btn color=\"primary\" type=\"submit\" large>Cadastrar</v-btn>\n                        </v-card-actions>\n\n                    </v-form>\n                </v-card>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\n    import { mapGetters, mapState } from 'vuex'\n    import usuarioRepository from \"../repositories/usuarioRepository\";\n\n    export default {\n        props: [\"to\", \"invalidToken\"],\n        data: () => ({\n            valid: true,\n            loading: false,\n            email: '',\n            password: '',\n            error: '',\n            requiredRule: [\n                v => !!v || 'Campo obrigatório'\n            ],\n        }),\n        created() {\n            if (this.invalidToken) {\n                this.error = 'Sua sessão expirou. Faça login novamente'\n            }\n        },\n        methods: {\n            onSubmit () {\n                this.loading = true\n                if (this.$refs.form.validate()) {\n                    let usuario = {\n                        nome: this.nome,\n                        email: this.email,\n                        password: this.password,\n                        habilitado: this.habilitado\n                    }\n                    usuarioRepository.save(usuario)\n                        .then(() => {\n                            this.$router.push('/home')\n                        })\n                }\n                this.loading = false\n            }\n        },\n        computed: {\n            ...mapState({\n                token: state => state.auth.token\n            })\n        }\n    };\n</script>\n\n<style scoped lang=\"css\">\n\n</style>\n"]}]}